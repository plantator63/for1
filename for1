M = int(input("Введите количество элементов в списке: "))
A = []
B = []

for i in range(1, M + 1):
    n = float(input("Введите элемент: "))
    A.append(n)
    
def lis_t(A):
    for element in A:
        if element > 5:
            print(element)
            B.append(element)
    return B

test = lis_t(A)

print("Список A - " + str(A))
print("Список В - " + str(test))
